{
    "version": 2,
    "build": {
        "env": {
            "NPM": "true"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/src/pages/index.tsx"
        },
        {
            "src": "/(.*).tsx",
            "dest": "/src/pages/$1.tsx"
        },
        {
            "src": "/(.*).tsx",
            "dest": "/index.tsx"
        }
    ]
}